<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="apps.cursos.com.mascotasbasedatos">

    <uses-permission
        android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="apps.cursos.com.mascotasbasedatos.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="apps.cursos.com.mascotasbasedatos.activity.MascotasFavoritasActivity"
            android:parentActivityName="apps.cursos.com.mascotasbasedatos.activity.MainActivity" />
        <activity android:name="apps.cursos.com.mascotasbasedatos.activity.ContactoActivity" />
        <activity android:name="apps.cursos.com.mascotasbasedatos.activity.AboutActivity"></activity>
    </application>

</manifest>